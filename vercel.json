{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "gh-pages": false
    }
  },
  "github": {
    "enabled": true,
    "silent": true
  },
  "ignoreCommand": "if [ \"$VERCEL_GIT_COMMIT_REF\" == \"gh-pages\" ]; then exit 0; else exit 1; fi"
}
