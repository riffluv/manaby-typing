{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "framework": "nextjs",  "git": {
    "deploymentEnabled": {
      "main": true,
      "gh-pages": false
    }
  },
  "github": {
    "enabled": true,
    "silent": true,
    "autoAlias": true,
    "productionBranch": "main",
    "deployOnPush": {
      "gh-pages": false,
      "main": true
    }
  },
  "headers": [
    {
      "source": "/sounds/(.*)\\.mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
